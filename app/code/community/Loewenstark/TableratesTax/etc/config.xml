        <Loewenstark_TableratesTax>
            <version>0.0.0.1</version>
        </Loewenstark_TableratesTax>
    </modules>
    <global>
        <helpers>
            <tablerates_tax>
                <class>Loewenstark_TableratesTax_Helper</class>
            </tablerates_tax>
        </helpers>
        <models>
            <tablerates_tax>
                <class>Loewenstark_TableratesTax_Model</class>
            </tablerates_tax>
            <shipping>

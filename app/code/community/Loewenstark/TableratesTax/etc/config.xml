                    <!-- Mage_Shipping_Model_Carrier_Tablerate //-->
                    <carrier_tablerate>Loewenstark_TableratesTax_Model_Shipping_Carrier_Tablerate</carrier_tablerate>
                </rewrite>
            </shipping>
        </models>
    </global>
</config>